{
  "name": "@vierweb/avataaars",
  "version": "3.0.7",
  "description": "React 19-compatible component for avataaars with optional animation features",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./options": {
      "types": "./dist/options/index.d.ts",
      "import": "./dist/options/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "docs": "typedoc",
    "start": "npm run build && npm run docs && npm run demo",
    "demo": "npm run build && cd demo && npm run dev",
    "demo:build": "npm run build && cd demo && npm run build",
    "demo:preview": "npm run demo:build && cd demo && npm run preview",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "keywords": [
    "react",
    "react18",
    "react-18",
    "react19",
    "react-19",
    "avatar",
    "avataaars",
    "animation",
    "animated-avatar",
    "avatar-generator"
  ],
  "repository": "git@github.com:donnybrilliant/avataaars.git",
  "homepage": "https://avataaars.vierweb.no",
  "author": "Daniel Vier <daniel.vier@gmail.com>",
  "contributors": [
    "Daniel Vier <daniel.vier@gmail.com>",
    "Fang-Pen Lin <hello@fangpenlin.com>",
    "Pablo Stanley"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "typedoc": "^0.28.15",
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "typescript-eslint": "^8.51.0",
    "globals": "^16.5.0"
  }
}
